<web-app>
	<servlet>
		<servlet-name>WebApp</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>WebApp</servlet-name>
		<url-pattern>/WebApp/*</url-pattern>
	</servlet-mapping>
</web-app>